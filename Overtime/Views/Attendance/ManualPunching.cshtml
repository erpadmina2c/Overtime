
@{
    ViewData["Title"] = "ManualPunching";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid" style="margin-top:10px" id="ManualPunching">
    <div class="row">

        <div class="col-md-3">
            <div class="form-group">
                <label class="control-label">Machine</label>
                <select id="machine" name="machine" class="form-control" asp-items="@(new SelectList(ViewBag.machineDetails,"machine_Id","machine_name"))" >
                    <option value="0">All</option>
                </select>
            </div>
        </div>
        <div class="col-md-2" align="center">
            <div class="form-group">
                <button type="button" onclick="addManualPunching();" class="btn btn-warning mt-4" >Punching</button>
            </div>
        </div>
    </div>
    <hr />
    <div id="container">

    </div>

</div>



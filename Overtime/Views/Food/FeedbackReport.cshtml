@{
    ViewData["Title"] = "Reports";
    Layout = "~/Views/Shared/_Layout.cshtml";
 }
<div class="container-fluid">
    <div class="card">
        <div class="card-body">
            <div class="page-header">
                <h3>Daily Food FeedBack Reports</h3>
            </div>
            <div class="row">
                <div class="col-md-1">
                    <div class="form-group">
                        <div class="form-group">
                            <label class="control-label">Select Date</label>
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label class="control-label">Date Range</label>
                        <input id="reportrange" class="form-control" style="background: #fff; cursor: pointer; padding: 5px 10px; border: 1px solid #ccc; width: 100%" />
                    </div>
                </div>
                <div class="col-md-1">
                    <div class="form-group" align="justify">
                        <label class="control-label"></label>
                        <input type="submit" onclick="GetFoodFeedBackDaily();" class="btn btn-primary" />
                    </div>
                </div>
            </div>
            <hr />
            <div class="card">
                <div class="card-body" id="container">


                </div>
            </div>
        </div>
    </div>
</div>
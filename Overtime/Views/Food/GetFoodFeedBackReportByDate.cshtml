@model System.Data.DataTable
@using System.Data;
@{
    Layout = null;
}
@if(ViewBag.dtcount ==0)
{
<div class="alert alert-info" role="alert">
    Info Nothing Found.. choose another date!
</div>
}
else{
<div class="table-responsive">
    <table class="table text-nowrap table-bordered " id="mytable" style="width:100%;">
        <thead>
            <tr>
                @foreach (DataColumn col in Model.Columns)
                {
                <th style="text-align: center;"> @col.ColumnName</th>

                }

            </tr>
        </thead>
        <tbody>
            @{
                foreach (DataRow row in Model.Rows)
                {
                <tr>

                    @foreach (DataColumn col in Model.Columns)
                        {
                    <td class="status_@row[col.ColumnName]" style="text-align: center;"> @row[col.ColumnName]</td>

                        }

                </tr>
                }
            }
        </tbody>
    </table>
</div>
}